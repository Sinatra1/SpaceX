<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="?attr/commonListSelectedRowColor" tools:color="@color/colorVeryDarkGray" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="?attr/commonListBackgroundColor" tools:color="@color/colorLightBlack" />
        </shape>
    </item>
</selector>